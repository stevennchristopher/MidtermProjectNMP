<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="match_parent"
    tools:context=".UsersProfile">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="0dp">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <ImageView
                android:id="@+id/icon_users4"
                android:layout_width="0dp"
                android:layout_height="30dp"
                app:layout_constraintEnd_toStartOf="@+id/icon_prefs4"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toEndOf="@+id/icon_create5"
                app:srcCompat="@drawable/user_icon"
                tools:layout_editor_absoluteY="579dp" />

            <ImageView
                android:id="@+id/icon_prefs4"
                android:layout_width="0dp"
                android:layout_height="30dp"
                android:layout_marginEnd="28dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:srcCompat="@drawable/prefs_icon"
                tools:layout_editor_absoluteY="579dp" />

            <ImageView
                android:id="@+id/icon_create5"
                android:layout_width="0dp"
                android:layout_height="30dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:srcCompat="@drawable/create_icon"
                tools:layout_editor_absoluteX="183dp" />

            <TextView
                android:id="@+id/txtUsername"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:elevation="4dp"
                android:fontFamily="@font/roboto_medium"
                android:text="AuroraHarmonix"
                android:textAlignment="center"
                android:textColor="#000000"
                android:textSize="20sp"
                app:layout_constraintTop_toTopOf="@+id/topBar2"
                tools:layout_editor_absoluteX="132dp" />

            <TextView
                android:id="@+id/txtCerbungUser"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:elevation="4dp"
                android:fontFamily="@font/roboto_medium"
                android:text="AuroraHarmonix Cerbung"
                android:textAlignment="center"
                android:textColor="#000000"
                android:textSize="20sp"
                app:layout_constraintTop_toBottomOf="@+id/imageView11"
                tools:layout_editor_absoluteX="91dp" />

            <ImageView
                android:id="@+id/topBar2"
                android:layout_width="465dp"
                android:layout_height="71dp"
                android:scaleType="centerCrop"
                app:srcCompat="@drawable/top_app_bar" />

            <ImageView
                android:id="@+id/icon_hamburg3"
                android:layout_width="35dp"
                android:layout_height="42dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="12dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/hamburger_icon" />

            <ImageView
                android:id="@+id/icon_notif2"
                android:layout_width="35dp"
                android:layout_height="42dp"
                android:layout_marginTop="12dp"
                android:layout_marginEnd="20dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/notif_icon" />

            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/topBar2"
                app:srcCompat="@drawable/rectangle" />

            <ImageView
                android:id="@+id/icon_home4"
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:srcCompat="@drawable/home_icon"
                tools:layout_editor_absoluteX="24dp" />

            <ImageView
                android:id="@+id/icon_following4"
                android:layout_width="0dp"
                android:layout_height="30dp"
                app:layout_constraintEnd_toStartOf="@+id/icon_create5"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toEndOf="@+id/icon_home4"
                app:srcCompat="@drawable/following_icon"
                tools:layout_editor_absoluteY="579dp" />

            <ImageView
                android:id="@+id/imageView9"
                android:layout_width="0dp"
                android:layout_height="122dp"
                android:layout_marginTop="17dp"
                app:layout_constraintTop_toTopOf="@+id/imageView4"
                app:srcCompat="@drawable/profile_pic"
                tools:layout_editor_absoluteX="97dp" />

            <ImageView
                android:id="@+id/imageView10"
                android:layout_width="0dp"
                android:layout_height="48dp"
                android:layout_marginTop="20dp"
                app:layout_constraintTop_toBottomOf="@+id/imageView9"
                app:srcCompat="@drawable/follow"
                tools:layout_editor_absoluteX="97dp" />

            <ImageView
                android:id="@+id/imageView11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="90dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/imageView10"
                app:srcCompat="@drawable/line" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>