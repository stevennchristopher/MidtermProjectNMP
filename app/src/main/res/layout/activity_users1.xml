<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Users1">

  <ImageView
      android:id="@+id/topBar"
      android:layout_width="412dp"
      android:layout_height="65dp"
      android:scaleType="centerCrop"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:srcCompat="@drawable/top_app_bar" />

  <TextView
      android:id="@+id/txtCerbung"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="116dp"
      android:layout_marginTop="20dp"
      android:layout_marginEnd="116dp"
      android:fontFamily="@font/roboto_medium"
      android:text="Users"
      android:textColor="#000000"
      android:textSize="20sp"
      app:layout_constraintEnd_toStartOf="@+id/icon_notif"
      app:layout_constraintStart_toEndOf="@+id/icon_back"
      app:layout_constraintTop_toTopOf="parent" />

  <ImageView
      android:id="@+id/icon_notif"
      android:layout_width="35dp"
      android:layout_height="42dp"
      android:layout_marginTop="12dp"
      android:layout_marginEnd="16dp"
      app:layout_constraintEnd_toEndOf="@+id/topBar"
      app:layout_constraintTop_toTopOf="parent"
      app:srcCompat="@drawable/notif_icon" />

  <ImageView
      android:id="@+id/icon_back"
      android:layout_width="35dp"
      android:layout_height="42dp"
      android:layout_marginStart="16dp"
      android:layout_marginTop="12dp"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:srcCompat="@drawable/hamburger_icon" />

  <ImageView
      android:id="@+id/navBar"
      android:layout_width="412dp"
      android:layout_height="99dp"
      android:scaleType="centerCrop"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:srcCompat="@drawable/navbar" />

  <ImageView
      android:id="@+id/icon_create"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="24dp"
      android:layout_marginEnd="24dp"
      android:layout_marginBottom="40dp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toStartOf="@+id/icon_users"
      app:layout_constraintStart_toEndOf="@+id/icon_following"
      app:srcCompat="@drawable/create_icon" />

  <ImageView
      android:id="@+id/icon_users"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginEnd="28dp"
      android:layout_marginBottom="40dp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toStartOf="@+id/icon_prefs"
      app:srcCompat="@drawable/user_icon" />

  <ImageView
      android:id="@+id/icon_prefs"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginEnd="24dp"
      android:layout_marginBottom="40dp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:srcCompat="@drawable/prefs_icon" />

  <TextView
      android:id="@+id/textFollowing"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="22dp"
      android:layout_marginTop="8dp"
      android:fontFamily="@font/roboto_medium"
      android:text="Following"
      android:textColor="#000000"
      android:textStyle="normal"
      app:layout_constraintStart_toEndOf="@+id/textHome"
      app:layout_constraintTop_toBottomOf="@+id/icon_following" />

  <ImageView
      android:id="@+id/icon_following"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="28dp"
      android:layout_marginBottom="40dp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintStart_toEndOf="@+id/icon_home"
      app:srcCompat="@drawable/following_icon" />

  <TextView
      android:id="@+id/textHome"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="27dp"
      android:layout_marginTop="8dp"
      android:fontFamily="@font/roboto_medium"
      android:text="Home"
      android:textColor="#000000"
      android:textStyle="normal"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/icon_home" />

  <TextView
      android:id="@+id/txtPrefs"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginTop="8dp"
      android:layout_marginEnd="29dp"
      android:fontFamily="@font/roboto_medium"
      android:text="Prefs"
      android:textColor="#000000"
      android:textStyle="normal"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/icon_prefs" />

  <TextView
      android:id="@+id/textCreate"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="24dp"
      android:layout_marginTop="8dp"
      android:layout_marginEnd="38dp"
      android:fontFamily="@font/roboto_medium"
      android:text="Create"
      android:textColor="#000000"
      android:textStyle="normal"
      app:layout_constraintEnd_toStartOf="@+id/txtUsers"
      app:layout_constraintHorizontal_bias="0.478"
      app:layout_constraintStart_toEndOf="@+id/textFollowing"
      app:layout_constraintTop_toBottomOf="@+id/icon_create" />

  <TextView
      android:id="@+id/txtUsers"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginTop="8dp"
      android:layout_marginEnd="37dp"
      android:fontFamily="@font/roboto_medium"
      android:text="Users"
      android:textColor="#000000"
      android:textStyle="normal"
      app:layout_constraintEnd_toStartOf="@+id/txtPrefs"
      app:layout_constraintTop_toBottomOf="@+id/icon_users" />

  <ImageView
      android:id="@+id/icon_home"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="24dp"
      android:layout_marginBottom="40dp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:srcCompat="@drawable/home_icon" />

  <ScrollView
      android:layout_width="0dp"
      android:layout_height="0dp"
      android:layout_marginTop="65dp"
      android:layout_marginBottom="99dp"
      android:fillViewport="true"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="1.0"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="1.0">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="-12dp"
        tools:layout_editor_absoluteY="13dp">

      <TextView
          android:id="@+id/textView"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_marginStart="64dp"
          android:layout_marginTop="30dp"
          android:fontFamily="@font/roboto_regular"
          android:text="Most Liked"
          android:textSize="16sp"
          app:layout_constraintStart_toStartOf="parent"
          app:layout_constraintTop_toTopOf="parent" />

      <TextView
          android:id="@+id/textView2"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_marginEnd="64dp"
          android:fontFamily="@font/roboto_regular"
          android:text="Following"
          android:textSize="16sp"
          app:layout_constraintBottom_toBottomOf="@+id/textView"
          app:layout_constraintEnd_toEndOf="parent"
          app:layout_constraintTop_toTopOf="@+id/textView"
          app:layout_constraintVertical_bias="0.0" />

      <View
          android:id="@+id/divider"
          android:layout_width="409dp"
          android:layout_height="1dp"
          android:layout_marginTop="12dp"
          android:background="?android:attr/listDivider"
          app:layout_constraintTop_toBottomOf="@+id/textView"
          tools:layout_editor_absoluteX="1dp" />

      <TextView
          android:id="@+id/textView3"
          android:layout_width="65sp"
          android:layout_height="2sp"
          android:background="@android:color/darker_gray"
          android:text="TextView"
          app:layout_constraintBottom_toTopOf="@+id/divider"
          app:layout_constraintEnd_toEndOf="@+id/textView"
          app:layout_constraintStart_toStartOf="@+id/textView" />

      <TextView
          android:id="@+id/textView4"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_marginStart="24dp"
          android:fontFamily="@font/roboto_regular"
          android:text="AhmadVeryCool\nsince 2019"
          app:layout_constraintBottom_toBottomOf="@+id/imageView"
          app:layout_constraintStart_toEndOf="@+id/imageView"
          app:layout_constraintTop_toTopOf="@+id/imageView" />

      <TextView
          android:id="@+id/textView6"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_marginEnd="30dp"
          android:fontFamily="@font/roboto_regular"
          android:text="823"
          android:textSize="18sp"
          android:textStyle="bold"
          app:layout_constraintBottom_toBottomOf="@+id/imageView"
          app:layout_constraintEnd_toEndOf="parent"
          app:layout_constraintTop_toTopOf="@+id/imageView" />

      <ImageView
          android:id="@+id/imageView"
          android:layout_width="70sp"
          android:layout_height="70sp"
          android:layout_marginStart="30dp"
          android:layout_marginTop="24dp"
          app:layout_constraintStart_toStartOf="parent"
          app:layout_constraintTop_toBottomOf="@+id/divider"
          app:srcCompat="@drawable/profile_pic" />

      <ImageView
          android:id="@+id/imageView2"
          android:layout_width="70sp"
          android:layout_height="70sp"
          android:layout_marginTop="16dp"
          app:layout_constraintStart_toStartOf="@+id/imageView"
          app:layout_constraintTop_toBottomOf="@+id/imageView"
          app:srcCompat="@drawable/profile_pic" />

      <TextView
          android:id="@+id/textView7"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_marginStart="24dp"
          android:fontFamily="@font/roboto_regular"
          android:text="ratsmacker999\nsince 2019"
          app:layout_constraintBottom_toBottomOf="@+id/imageView2"
          app:layout_constraintStart_toEndOf="@+id/imageView2"
          app:layout_constraintTop_toTopOf="@+id/imageView2" />

      <TextView
          android:id="@+id/textView9"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_marginEnd="30dp"
          android:fontFamily="@font/roboto_regular"
          android:text="23"
          android:textSize="18sp"
          android:textStyle="bold"
          app:layout_constraintBottom_toBottomOf="@+id/imageView2"
          app:layout_constraintEnd_toEndOf="parent"
          app:layout_constraintTop_toTopOf="@+id/imageView2" />
    </androidx.constraintlayout.widget.ConstraintLayout>
  </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>